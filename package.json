    "lodash": "4.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.21",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.3.13",
    "chai": "3.4.1",
    "chai-spies": "0.7.1",
    "docco": "0.7.0",
